{
  "name": "classic-eq-mapper",
  "version": "0.1.0",
  "description": "Displays your location on classic EQ maps in your browser via log parsing",
  "author": "ssk101",
  "license": "GPL",
  "main": "app.js",
  "scripts": {
    "build": "browserify ./src -o ./build/bundle.js",
    "watch": "watchify ./src -o ./build/bundle.js",
    "dev-server": "./node_modules/.bin/nodemon app.js",
    "server": "node app.js",
    "start": "npm run watch & npm run server",
    "dev": "npm run watch & npm run dev-server"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "babel-plugin-import-postcss": "^2.0.0",
    "babelify": "^10.0.0",
    "construct-style-sheets-polyfill": "^2.2.10",
    "postcss-preset-env": "^6.7.0",
    "socket.io": "^2.3.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.3",
    "pug": "^2.0.4",
    "tail": "^2.0.3",
    "watchify": "^3.11.1"
  }
}
